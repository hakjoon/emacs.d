(:name pymacs
       :description "Interface between Emacs Lisp and Python"
       :type git
       :url "http://github.com/hakjoon/emacs-pymacs.git"
       :features "pymacs"
       :post-init (lambda ()
		   (setenv "PYTHONPATH"
			   (concat
			    (getenv "PYTHONPATH") path-separator
			    (expand-file-name (concat el-get-dir "pymacs"))))))